
@{
    ViewBag.Title = "Predict_Marks";
}

<link href="~/Content/CreateUser/css/createuser.css" rel="stylesheet" />

<link href="~/Content/ViewAllUserAssets/css/sb-admin-2.css" rel="stylesheet" />
<main>
    <div class="container">
        <header>Marks Prediction</header>
        <form style="min-height:150px" onsubmit="getstudent();return false">
            <div class="form first">
                <div class="details personal">
                    <span class="title">Predict Marks for Speech Therapy Assessment</span>

                    <div class="fields">
                        <div class="input-field">

                            <input id="_gr_no" type="text" placeholder="Enter Student GR No." required />

                        </div>
                        <div class="input-field">
                            <button type="submit" class="backBtn">
                                <span class="btnText">Submit</span>
                                <i style="transform:none !important" class="uil uil-navigator"></i>
                            </button>



                        </div>
                        <div class="input-field" style="visibility:hidden">

                            <label>Full Name</label>
                            <input type="text" placeholder="Enter Student GR No." />


                        </div>












                        <div class="input-field" style="visibility:hidden">
                            <label>Full Name</label>
                            <input type="text" placeholder="Enter Student GR No." />

                        </div>

                    </div>


                </div>

            </div>
        </form>


        <div id="tabless" class="container-fluid" style="visibility:hidden">

            <div class="row">

                <div class="col-lg-12 m-b30">
                    <div class="widget-box">

                        <div class="card shadow mb-4">
                            <div class="card-body">
                                <div class="table-responsive">
                                    <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                        <thead>
                                            <tr class="text-center">
                                                <th>ID</th>
                                                <th>First Name</th>
                                                <th>Last Name</th>
                                                <th>Father Name</th>
                                                <th>Gender</th>
                                                <th>Disability</th>

                                            </tr>
                                        </thead>

                                        <tbody id="SetStudentData">
                                        </tbody>
                                    </table>
                                </div>
                                <button id="button_change" onclick="create_csv()" class="backBtn">
                                    <span class="btnText">Create CSV</span>

                                </button>
                            </div>
                            @*    Your Profile Views Chart END*@
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <script src="https://cdn.jsdelivr.net/npm/apexcharts"></script>
        <div class="content-data">
            <div class="head">

            </div>
            <div class="chart">
                <div id="chart6"></div>
            </div>
        </div>
    </div>
   


</main>



<script src="~/Content/Adminassets/js/predict_marks.js"></script>